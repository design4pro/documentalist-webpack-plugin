{
  "name": "documentalist-webpack-plugin",
  "version": "0.2.0",
  "description": "Documentalist WebPack Plugin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "tslint --project .",
    "release": "release-me --release-count=0",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc --project . && echo 'OK'",
    "watch": "tsc --project . --watch"
  },
  "homepage": "https://github.com/design4pro/documentalist-webpack-plugin#readme",
  "devDependencies": {
    "@types/node": "^8.0.44",
    "codecov": "^2.2.0",
    "commitizen": "^2.9.6",
    "cz-release-me": "^1.3.2",
    "nyc": "^11.0.3",
    "release-me": "^1.4.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "better-handlebars": "github:wmeldon/better-handlebars",
    "documentalist": "^0.0.8",
    "highlights": "^3.0.1",
    "language-less": "github:atom/language-less",
    "language-typescript": "github:giladgray/language-typescript#10.1.15",
    "marked": "^0.3.6",
    "sassdoc": "^2.3.0"
  },
  "engines": {
    "node": ">=6.1"
  },
  "config": {
    "commitizen": {
